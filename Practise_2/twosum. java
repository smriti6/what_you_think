Class twosum {
   Public static void main (String args []) 
   {
      int arr []={2,1,3,5,4};
       int t= 4;
       int a=-1;
       for(int x=0;x<arr.length;x++) 
       {
          for(int y=0;y<arr.length;y++) 
          {
             if(x==y) 
             {
                continue;
             }
             else 
             {
                if(arr[x]+arr[y]==t) 
                {
                   System.out.println(arr[x]);
                   a=arr[y];
                }

             }
          }
       }

       System.out.println(a);
   }
}
