Class twosum {
   Public static void main (String args []) 
   {
      int arr []={2,1,3,5,4};
       int t= 4;
       for(int x=0;x<arr.length-1;x++) 
       {
          for(int y=x+1;y<arr.length;y++) 
          {
                if(arr[x]+arr[y]==t) 
                {
                   System.out.println(arr[x]+""+arr[y])
                }
          }
       }
   }
}
