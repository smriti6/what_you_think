Class binarysearch
{
   Public static void main (string args []) 
   {
     int arr []={2,4,6,7,8,9};
     int start =0;
     int end =arr.length;
     int mid = start(end-start)/2;
     while(start<=end) 
     {
        if(target==mid)
            return mid;
        if(target<mid) 
            end=mid-1;
        if(target>mid) 
            start=mid+1;
     }
   }
}
